{
  "name": "vue2-with-vite",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "format": "npm run --prefix node_modules/@7span/prettier-config 7span-prettier-format",
    "lint": "npx eslint --fix"
  },
  "prettier": "@7span/prettier-config",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "floating-vue": "^1.0.0-beta.18",
    "vue": "^2.7.7",
    "vue-router": "^3.5.4"
  },
  "devDependencies": {
    "@vitejs/plugin-legacy": "^2.0.0",
    "@vitejs/plugin-vue2": "^1.1.2",
    "autoprefixer": "^10.4.12",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.18",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.0",
    "terser": "^5.14.2",
    "vite": "^3.0.2",
    "husky": "^8.0.3"
  }
}
